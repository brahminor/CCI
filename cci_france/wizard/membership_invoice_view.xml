<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_membership_invoice_form" model="ir.ui.view">
        <field name="name">membership.invoice.form</field>
        <field name="model">membership.invoice</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="membership.view_membership_invoice_view"/>
        <field name="priority" eval="60"/>
        <field name="arch" type="xml">
        <xpath expr="//field[@name='product_id']" position="before">
            <field name="partner_id"/>
        </xpath>
        </field>
    </record>
</odoo>
